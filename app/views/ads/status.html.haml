%style
  body{background-color: #333333; color: #aaaaaa;}
Last updated:
=Ad.all.order(updated_at: :asc).last.updated_at.to_formatted_s(:short)
-@ads.each do |ad|
  -if ad.passed? || ad.seo?
    .passed
      =render partial:'ads/row', locals: {ad: ad}
  -else
    .failed
      =render partial:'ads/row', locals: {ad: ad}